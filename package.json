{
  "name": "@dathaplus/starter-kit-design-system",
  "version": "0.1.12-alpha.1",
  "private": false,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "dist": "dist"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "build": "tsc --build \"./tsconfig.json\"",
    "build-storybook": "build-storybook -s public",
    "clean": "rm -rf ./dist",
    "compile-icons": "npx @svgr/cli -d src/icons src/icons --typescript",
    "copy-fonts": "copyfiles --up 1 src/config/fonts/* dist/",
    "eject": "react-scripts eject",
    "format": "prettier --write \"./**/*.{tsx,ts,js,json,yml,yaml}\" \"./.{lintstagedrc,nycrc,prettierrc}\"",
    "postbuild": "npm run copy-fonts",
    "husky:install": "husky install",
    "prebuild": "npm run clean && npm run compile-icons",
    "prepare": "npm run prebuild && npm run rollup",
    "rollup": "rollup -c",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 6006 -s public",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0",
    "react-is": ">=18.2.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.4",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/node-logger": "^6.5.10",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "0.0.13",
    "@svgr/cli": "^6.3.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.45",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.25",
    "babel-plugin-named-exports-order": "0.0.2",
    "copyfiles": "^2.4.1",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "rollup": "^2.77.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-import-css": "^3.0.3",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "styled-components": "^5.3.5",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0"
  }
}
